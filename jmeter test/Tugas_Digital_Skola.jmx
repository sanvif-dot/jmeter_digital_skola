          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion">
            <stringProp name="scriptLanguage">javascript</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Get HTTP response body
var response = prev.getResponseDataAsString();

// Parse once
var json = JSON.parse(response);

// Log for debug
log.info(&quot;First name: &quot; + json.data.first_name);

// Assert first name
if (json.data.first_name !== &quot;Janet&quot;) {
    throw &quot;Expected first_name = Janet but got &quot; + json.data.first_name;
}

// Optional: assert HTTP 200
if (prev.getResponseCode() !== &quot;200&quot;) {
    throw &quot;Expected status code 200 but got &quot; + prev.getResponseCode();
}
</stringProp>
